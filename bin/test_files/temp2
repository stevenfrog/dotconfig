========= package =================
com.emc.gs.tools.srf.model.dpad

========= class ===================
NetworkerDesign

========= fields ==================
boolean                            serverSelected
boolean                            advancedBuTechSelected
boolean                            storageNodesSelected
boolean                            fsClientBuSelected
boolean                            mediaDevicesSelected
boolean                            appBuSelected
NetworkerServer                    server
NetworkerAdvancedBackupTechnology  advancedBuTech
NetworkerStorageNodes              storageNodes
NetworkerFileSystemClientBackups   fsClientBu
NetworkerMediaDevices              mediaDevices
NetworkerApplicationBackups        appBu

----- com.emc.gs.tools.srf.model.dpad -----
===== NetworkerDesign =====
boolean                            |  serverSelected          |  srv_selected               |  BOOLEAN NOT NULL
boolean                            |  advancedBuTechSelected  |  advanced_bu_tech_selected  |  BOOLEAN NOT NULL
boolean                            |  storageNodesSelected    |  storage_nodes_selected     |  BOOLEAN NOT NULL
boolean                            |  fsClientBuSelected      |  fs_client_bu_selected      |  BOOLEAN NOT NULL
boolean                            |  mediaDevicesSelected    |  media_devices_selected     |  BOOLEAN NOT NULL
boolean                            |  appBuSelected           |  app_bu_selected            |  BOOLEAN NOT NULL
NetworkerServer                    |  server                  |  srv_id                     |  ManyToOne
NetworkerAdvancedBackupTechnology  |  advancedBuTech          |  advanced_bu_tech_id        |  ManyToOne
NetworkerStorageNodes              |  storageNodes            |  storage_nodes_id           |  ManyToOne
NetworkerFileSystemClientBackups   |  fsClientBu              |  fs_client_bu_id            |  ManyToOne
NetworkerMediaDevices              |  mediaDevices            |  media_devices_id           |  ManyToOne
NetworkerApplicationBackups        |  appBu                   |  app_bu_id                  |  ManyToOne

CREATE TABLE NetworkerDesign (
    id BIGINT AUTO_INCREMENT NOT NULL PRIMARY KEY,
    srv_selected BOOLEAN NOT NULL,
    advanced_bu_tech_selected BOOLEAN NOT NULL,
    storage_nodes_selected BOOLEAN NOT NULL,
    fs_client_bu_selected BOOLEAN NOT NULL,
    media_devices_selected BOOLEAN NOT NULL,
    app_bu_selected BOOLEAN NOT NULL,
    srv_id BIGINT,
    advanced_bu_tech_id BIGINT,
    storage_nodes_id BIGINT,
    fs_client_bu_id BIGINT,
    media_devices_id BIGINT,
    app_bu_id BIGINT,
    FOREIGN KEY (srv_id) REFERENCES NetworkerServer(id),
    FOREIGN KEY (advanced_bu_tech_id) REFERENCES NetworkerAdvancedBackupTechnology(id),
    FOREIGN KEY (storage_nodes_id) REFERENCES NetworkerStorageNodes(id),
    FOREIGN KEY (fs_client_bu_id) REFERENCES NetworkerFileSystemClientBackups(id),
    FOREIGN KEY (media_devices_id) REFERENCES NetworkerMediaDevices(id),
    FOREIGN KEY (app_bu_id) REFERENCES NetworkerApplicationBackups(id)
);




